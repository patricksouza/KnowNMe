{
  "expo": {
    "name": "KnowNMe",
    "description": "An app made to know you!",
    "slug": "KnowNMe",
    "privacy": "public",
    "version": "1.2.0",
    "orientation": "portrait",
    "platforms": [
      "android"
    ],
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#F4717F"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "package": "com.codevapps.KnowNMe"
    },
    "androidStatusBar": {
      "barStyle": "light-content",
      "backgroundColor": "#100F12"
    }
  }
}
